cd SiertSat
if ! [ -e solver-reducer ]; then
    hg clone https://bitbucket.org/siert/solver-reducer .
fi
hg update -rff5a14bf8cb9

mkdir -p glucored
cd glucored
for i in ../solver-reducer/*.{cc,h}; do ln -sf $i; done
cd ..

mkdir -p minired
cd minired
for i in ../solver-reducer/*.{cc,h}; do ln -sf $i; done
cd ..
